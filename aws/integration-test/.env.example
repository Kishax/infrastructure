# aws settings
AWS_REGION=your-aws-region
AWS_ACCOUNT_ID=your-aws-account-id
AWS_PROFILE=your-aws-profile

# cloudformation settings
AWS_STACK_NAME=your-cloudformation-stack-name

# api gateway settings
AWS_API_GATEWAY_ID=your-api-gateway-id
AWS_API_GATEWAY_RESOURCE_ID=your-api-gateway-resource-id
AWS_API_GATEWAY_STAGE=prod

# API Gateway Resource IDs
AWS_API_GATEWAY_WEB_TO_MC_RESOURCE_ID=your-web-to-mc-resource-id
AWS_API_GATEWAY_MC_TO_WEB_RESOURCE_ID=your-mc-to-web-resource-id
AWS_API_GATEWAY_DISCORD_RESOURCE_ID=your-discord-resource-id

# lambda settings
AWS_LAMBDA_FUNCTION_NAME=your-lambda-function-name

# ecr settings
AWS_ECR_REPO_DISCORD_BOT_NAME=your-discord-bot-repo-name
AWS_ECR_REPO_GATHER_BOT_NAME=your-gather-bot-repo-name
AWS_ECR_REPO_WEB_BOT_NAME=your-web-bot-repo-name

# each markdown settings
AWS_IAM_ROLE_NAME_FOR_API_GATEWAY=your-api-gateway-role-name
AWS_IAM_ROLE_NAME_FOR_API_GATEWAY_ACCESS_KEY=your-api-gateway-access-key
AWS_IAM_POLICY_NAME_FOR_API_GATEWAY=your-api-gateway-policy-name

# API Gateway Authentication (for testing)
AWS_ACCESS_KEY_ID=your-api-gateway-access-key
AWS_SECRET_ACCESS_KEY=your-api-gateway-secret-access-key

AWS_VPC_ID=your-vpc-id
AWS_VPC_NAME=your-vpc-name
AWS_PRIVATE_SUBNET_IDS=your-private-subnet-id1,your-private-subnet-id2
AWS_PUBLIC_SUBNET_IDS=your-public-subnet-id1,your-public-subnet-id2

# SSL証明書設定（ACM）
AWS_SSL_CERTIFICATE_ARN=your-ssl-certificate-arn

# API Gateway URL for bidirectional communication testing
API_GATEWAY_URL=https://your-api-gateway-resource-id.execute-api.ap-northeast-1.amazonaws.com/prod


# Discord settings (placeholder for testing)
DISCORD_CHANNEL_ID=your-discord-channel-id

# 各通信パターン用SQSキュー名（URLは自動生成）
AWS_SQS_DISCORD_QUEUE_NAME=your-discord-queue-name
AWS_SQS_MC_TO_WEB_QUEUE_NAME=your-mc-to-web-queue-name
AWS_SQS_WEB_TO_MC_QUEUE_NAME=your-web-to-mc-queue-name

# 各通信パターン用DLQキュー名
AWS_SQS_DISCORD_DLQ_NAME=your-discord-dlq-name
AWS_SQS_MC_TO_WEB_DLQ_NAME=your-mc-to-web-dlq-name
AWS_SQS_WEB_TO_MC_DLQ_NAME=your-web-to-mc-dlq-name

# 各通信パターン用API Gatewayリソースパス
AWS_API_GATEWAY_DISCORD_RESOURCE_PATH=/your-discord-resource-path
AWS_API_GATEWAY_WEB_TO_MC_RESOURCE_PATH=/your-web-to-mc-resource-path
AWS_API_GATEWAY_MC_TO_WEB_RESOURCE_PATH=/your-mc-to-web-resource-path
